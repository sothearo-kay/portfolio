{
  "rewrites": [
    {
      "source": "/script.js",
      "destination": "https://cloud.umami.is/script.js",
      "has": [{"type": "host", "value": "analytics.sothearo.dev"}]
    },
    {
      "source": "/api/(.*)",
      "destination": "https://cloud.umami.is/api/$1",
      "has": [{"type": "host", "value": "analytics.sothearo.dev"}]
    }
  ],
  "headers": [
    {
      "source": "/script.js",
      "has": [{"type": "host", "value": "analytics.sothearo.dev"}],
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "has": [{"type": "host", "value": "analytics.sothearo.dev"}],
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://sothearo.dev/$1",
      "permanent": false,
      "has": [{"type": "host", "value": "analytics.sothearo.dev"}]
    }
  ]
}